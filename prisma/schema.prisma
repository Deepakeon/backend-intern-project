// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Climate {
  HOT
  HUMID
  RAINY
  COLD
}

model WeatherSync{
  id Int @id @default(autoincrement())
  climate Climate
  areaCode Int
  temperature Float
  humidity Float
  chancesOfRain Float
}